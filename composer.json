{
  "name": "furious/container",
  "description": "PHP 7.4 implementation of DI",
  "license": "MIT",
  "keywords": ["di", "container", "dependency injection", "php", "php 7.4", "psr 11", ""],
  "authors": [
    {
      "name": "Arslanov Rasul",
      "email": "rasularslanoov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Furious\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Furious\\Container\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}